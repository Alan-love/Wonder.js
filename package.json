{
  "name": "wonder.js",
  "version": "1.0.0-alpha.12.1",
  "authors": "Wonder",
  "description": "3d webgl engine",
  "homepage": "https://github.com/Wonder-Technology/Wonder.js",
  "license": "MIT",
  "main": "",
  "scripts": {
    "start": "node ./server/app.js",
    "codecov": "cat test/coverage/lcov.info | codecov",
    "watch": "bsb -clean-world -make-world -w",
    "build": "bsb -clean-world -make-world",
    "buildAll": "npm run build && gulp build",
    "test": "jest --config jest.json",
    "test:coverage": "jest --config jest_coverage.json",
    "test:ci": "jest --maxWorkers=4 --config jest_coverage.json",
    "test:render": "npm run build && gulp testRenderInLocal",
    "test:pf": "npm run build && gulp testPerformanceInLocal",
    "test:watch": "jest --watch --config jest.json",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bs-platform": "2.1.0",
    "codecov.io": "^0.1.6",
    "cz-customizable": "^5.2.0",
    "express": "^4.16.1",
    "gulp": "^3.9.1",
    "gulp-git": "^2.5.0",
    "gulp-sync": "^0.1.4",
    "http": "^0.0.0",
    "rollup": "^0.51.5",
    "puppeteer": "^0.13.0",
    "wonder-benchmark": "^0.1.30",
    "wonder-bs-jest": "latest",
    "wonder-bs-sinon": "latest",
    "wonder-generate-index": "latest",
    "wonder-glsl-compiler": "latest",
    "wonder-render-test": "^0.1.9"
  },
  "dependencies": {
    "bs-json": "^0.2.3",
    "wonder-commonlib": "latest"
  },
  "resolutions": {
    "wonder-commonlib": "0.2.17",
    "wonder-bs-jest": "0.2.17"
  }
}