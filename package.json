{
  "name": "wonder.js",
  "version": "1.0.0-alpha.16",
  "authors": "Wonder",
  "description": "3d webgl engine",
  "homepage": "https://github.com/Wonder-Technology/Wonder.js",
  "repository": "git@github.com:Wonder-Technology/Wonder.js.git",
  "license": "MIT",
  "main": "dist/wd.js",
  "scripts": {
    "start": "node ./server/app.js",
    "watch": "bsb -clean-world -make-world -w",
    "build": "bsb -clean-world -make-world",
    "buildAll": "gulp createShaderChunkSystemFile && gulp generateIndex && npm run build && gulp rollup",
    "buildAll:ci": "npm run build && gulp createShaderChunkSystemFile && gulp generateIndex && npm run build && gulp rollup",
    "test": "jest --config jest.json",
    "test:coverage": "npm run build && jest --config jest_coverage.json",
    "test:ci": "npm run buildAll:ci && jest --maxWorkers=4 --config jest_coverage.json",
    "test:render": "npm run build && gulp testRenderInLocal",
    "test:renderCI": "npm run build && gulp testRenderInCI",
    "test:pf": "npm run build && gulp testPerformanceInLocal",
    "test:pfCI": "npm run build && gulp testPerformanceInCI",
    "test:watch": "jest --watch --config jest.json",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "e2e:upgradeBaseCommitId": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bs-platform": "^2.1.0",
    "cz-customizable": "^5.2.0",
    "express": "^4.16.1",
    "gulp": "^3.9.1",
    "gulp-git": "^2.5.0",
    "gulp-sync": "^0.1.4",
    "http": "^0.0.0",
    "rollup": "^0.51.5",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^2.0.0",
    "wonder-benchmark": "^0.1.45",
    "wonder-bs-jest": "latest",
    "wonder-bs-sinon": "^0.1.6",
    "wonder-generate-index": "latest",
    "wonder-glsl-compiler": "^0.1.4",
    "wonder-render-test": "^0.1.14",
    "puppeteer": "^0.13.0"
  },
  "dependencies": {
    "bs-fetch": "^0.2.1",
    "bs-json": "^0.2.3",
    "bs-most": "^0.1.1",
    "wonder-commonlib": "^0.2.19",
    "wonder-log": "^0.1.1"
  },
  "resolutions": {
    "wonder-commonlib": "0.2.19",
    "wonder-bs-jest": "0.2.17",
    "wonder-bs-puppeteer": "^0.1.1"
  }
}
